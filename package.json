{
  "name": "cineo_backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node cineo_backend/server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:frontend": "jest --testEnvironment=jsdom --testMatch='**/cineo_tests/frontend/**/*.test.js'",
    "test:backend": "jest --testEnvironment=node --testMatch='**/cineo_tests/backend/**/*.test.js'"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "html"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/cineo_tests/backend/**/*.test.js",
      "**/cineo_test/frontend/**/*.test.js"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^@supabase/supabase-js$": "<rootDir>/cineo_tests/__mocks__/supabase.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.47.10",
    "axios": "^1.7.9",
    "cineo_backend": "file:",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mysql2": "^3.11.5",
    "safer-buffer": "^2.1.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "msw": "^2.7.0",
    "nock": "^13.5.6",
    "nodemon": "^2.0.22",
    "supertest": "^7.0.0"
  },
  "type": "commonjs"
}
